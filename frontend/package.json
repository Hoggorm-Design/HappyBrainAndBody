    "framer-motion": "^11.11.9",
    "he": "^1.2.0",
    "helmet": "^8.0.0",
    "history": "^5.3.0",
    "html-escaper": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.1"
  },
